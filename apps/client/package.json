		"axios": "^1.11.0",
		"i18next": "^25.3.6",
		"i18next-browser-languagedetector": "^8.2.0",
		"i18next-http-backend": "^3.0.2",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-error-boundary": "^6.0.0",
		"react-hook-form": "^7.62.0",
		"react-i18next": "^15.6.1",
		"react-router-dom": "^7.7.1",
		"zustand": "^5.0.8"
	}
