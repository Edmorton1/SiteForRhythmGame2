{
	"name": "client",
	"version": "1.0.0",
	"type": "module",
	"private": true,
	"scripts": {
		"watch": "tsc --noEmit --watch",
		"lint": "eslint \"src/**/*.{ts,tsx}\" ",
		"dev": "vite",
		"build": "tsc -b && vite build",
		"preview": "vite preview",
		"test": "jest --config jest.config.ts",
		"test:react": "jest --config jest.react.config.ts",
		"test:watch": "jest --watch --config jest.unit.config.ts",
		"test:react:watch": "jest --watch --config jest.react.config.ts",
		"test:cov": "jest --coverage --config jest.unit.config.ts",
		"test:react:cov": "jest --coverage --config jest.react.config.ts",
		"test:e2e": "cypress open"
	},
	"devDependencies": {
		"@tanstack/eslint-plugin-query": "^5.83.1",
		"@tanstack/react-query-devtools": "^5.85.5",
		"@testing-library/jest-dom": "^6.6.4",
		"@testing-library/react": "^16.3.0",
		"@types/react": "^19.1.9",
		"@types/react-dom": "^19.1.7",
		"@vitejs/plugin-react": "^4.6.0",
		"cypress": "^14.5.3",
		"eslint-plugin-cypress": "^5.1.0",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.20",
		"jest-environment-jsdom": "^30.0.5",
		"vite": "^7.0.4"
	},
	"dependencies": {
		"@hookform/resolvers": "^5.2.1",
