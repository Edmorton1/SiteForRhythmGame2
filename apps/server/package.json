{
	"name": "server",
	"version": "1.0.0",
	"type": "commonjs",
	"scripts": {
		"watch": "tsc --noEmit --watch",
		"dev": "nodemon ./src/web-server/index.ts",
		"dev:auth": "nodemon ./src/microservices/services/auth/index.ts",
		"dev:tracks": "nodemon ./src/microservices/services/tracks/index.ts",
		"start": "node dist/apps/server/src/main",
		"build": "esbuild src/index.ts --bundle --outfile=dist/index.js --platform=node",
		"format": "prettier --write \"src/**/*.ts\"",
		"lint": "eslint \"{src}/**/*.ts\" ",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:e2e": "jest --config ./jest.e2e.ts"
	},
	"bin": {
		"cli": "./cli/cli.js"
	},
	"devDependencies": {
		"@asteasolutions/zod-to-openapi": "^8.1.0",
		"@types/bcrypt": "^6.0.0",
		"@types/cookie-parser": "^1.4.9",
		"@types/express": "^5.0.3",
		"@types/express-session": "^1.18.2",
		"@types/multer": "^2.0.0",
		"@types/node": "^22.10.7",
		"@types/passport": "^1.0.17",
		"@types/passport-google-oauth20": "^2.0.16",
		"@types/pg": "^8.15.5",
		"@types/supertest": "^6.0.2",
		"@types/swagger-ui-express": "^4.1.8",
		"commander": "^14.0.1",
		"esbuild": "^0.25.9",
		"nodemon": "^3.1.10",
		"pino": "^9.9.0",
		"pino-pretty": "^13.1.1",
		"supertest": "^7.0.0",
		"ts-loader": "^9.5.2",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.7.3"
	},
	"dependencies": {
		"bcrypt": "^6.0.0",
		"cookie-parser": "^1.4.7",
		"dotenv": "^17.2.1",
		"express": "^5.1.0",
		"express-session": "^1.18.2",
		"helmet": "^8.1.0",
		"inversify": "^7.9.1",
		"ioredis": "^5.7.0",
		"kafkajs": "^2.2.4",
		"kysely": "^0.28.4",
		"multer": "^2.0.2",
		"passport": "^0.7.0",
		"passport-google-oauth20": "^2.0.0",
		"pg": "^8.16.3",
		"reflect-metadata": "^0.2.2",
		"swagger-ui-express": "^5.0.1"
	}
}
